{
  "name": "vue-js",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --port 3003 --open",
    "lint": "vue-cli-service lint",
    "deploy": "serve -s build",
    "predeploy": "npm run build && make setup_release",
    "start": "REACT_APP_RELEASE=`sentry-cli releases propose-version` vue-cli-service serve --port 3003 --open",
    "build": "REACT_APP_RELEASE=`sentry-cli releases propose-version` vue-cli-service build"
  },

  
  "dependencies": {
    "@sentry/browser": "^5.4.3",
    "@sentry/integrations": "^5.4.2",
    "@sentry/cli": "^1.37.0",
    "core-js": "^2.6.5",
    "vue": "^2.6.10"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.8.0",
    "@vue/cli-plugin-eslint": "^3.8.0",
    "@vue/cli-service": "^3.8.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
